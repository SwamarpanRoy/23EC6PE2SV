class Packet;

  rand bit [7:0] data;

  virtual function void print();
    $display("Normal: %h", data);
  endfunction

endclass


class BadPacket extends Packet;

  virtual function void print();
    $display("ERROR: %h", data);
  endfunction

endclass


module tb;

  Packet p;
  BadPacket bad = new();

  initial begin


    p = bad;
    p.randomize();
    p.print();

    $finish;

  end

endmodule
